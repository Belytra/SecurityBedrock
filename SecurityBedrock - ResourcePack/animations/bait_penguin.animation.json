{
	"format_version": "1.8.0",
	"animations": {
		"animation.bait_penguin.walk": {
			"loop": true,
			"animation_length": 2,
			"bones": {
				"body": {
					"rotation": [0, 0, "math.cos(query.anim_time * 360) * 5"]
				},
				"right_l": {
					"rotation": ["math.sin(query.anim_time * 360) * 5", 0, 0]
				},
				"left_l": {
					"rotation": ["math.sin(query.anim_time * 360) * -5", 0, 0]
				},
				"right_a": {
					"rotation": [0, 0, "10 + math.cos(query.anim_time * 360) * 5"]
				},
				"left_a": {
					"rotation": [0, 0, "-10 + math.cos(query.anim_time * 360) * -5"]
				}
			}
		}
	}
}